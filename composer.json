{
    "name": "janwebdev/object-mapper",
    "description": "Simple object mapping.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "janwebdev",
            "email": "janwebdev@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Janwebdev\\ObjectMapper\\": "src/"
        },
        "files": [
            "src/Parser/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Janwebdev\\ObjectMapper\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "laminas/laminas-hydrator": "^4.3",
        "psr/simple-cache": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.1",
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "cs-fix": ["php-cs-fixer fix -v"],
        "test": "phpunit tests"
    }
}
